@using TravelTripProje.Models.Siniflar
@model List<Blog>
<div class="padding1">
    <h4>Tarihi Yerler</h4>
    <div class="box-left">
        @foreach(var x in Model)
        {
        <div class="bot-img">
            <a href="/Blog/BlogDetay/@x.ID"><img src="@x.BlogImage"style="width:120px; height:85px;" alt="" /></a>
        </div>
        <div class="bot-desc">
            <p>@x.Aciklama.Substring(0,80)...</p>
        </div>
        <div class="clear"></div>                      
        }
    </div>
    @Html.Action("Partial5","Default")
</div>
